{
  "name": "mi-note-vue",
  "displayName": "TODOs",
  "version": "1.1.0",
  "description": "A sample Apache Cordova application that responds to the deviceready event.",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "c:s": "cordova serve",
    "c:r:a": "yarn build && cordova run android --device",
    "c:rq": "cordova requirements",
    "clean": "rm -rf node_modules/ plugins/ platforms/ package-lock.json yarn.lock",
    "json-serve": "json-server db.json --watch --port 3001"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ./scripts/pre-commit.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.0",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^3.0.1",
    "framework7": "^4.3.0",
    "framework7-vue": "^4.3.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "json-server": "^0.14.2",
    "vue-template-compiler": "^2.6.10",
    "husky": "^1.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-camera": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
