{
  "name": "mi-note-vue",
  "displayName": "MI NOTE",
  "version": "1.2.0",
  "private": true,
  "description": "A sample Framework7-Vue application Cordova application that responds to the deviceready event.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf node_modules/ plugins/ platforms/ package-lock.json yarn.lock",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "c:s": "cordova serve",
    "c:r:a": "yarn build && cordova run android --device",
    "c:rq": "cordova requirements",
    "res": "crgen -p android,ios"
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh ./scripts/pre-commit.sh",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cordova-android": "^8.0.0",
    "cordova-ios": "^5.0.0",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.6.5",
    "framework7": "^4.3.0",
    "framework7-vue": "^4.3.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cordova-res-generator": "^0.4.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.10",
    "husky": "^1.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-angular": "^7.5.0"
  },
  "keywords": [
    "cordova",
    "vue",
    "framework7"
  ],
  "author": "NieLamu",
  "license": "MIT",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-camera": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
